{
  "maxLogEntries": 1000,
  "file": {
    "path": "/var/log/dns_logs.json",
    "enabled": true
  },
  "http": {
    "endpoint": "http://example.com/logs",
    "method": "POST",
    "headers": {
      "Authorization": "Bearer abc123"
    },
    "enabled": true
  },
  "syslog": {
    "address": "127.0.0.1",
    "port": 514,
    "protocol": "UDP",
    "enabled": true
  }
}