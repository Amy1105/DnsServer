{
    "maxLogEntries": 10000,
    "targets": [
        {
            "type": "file",
            "enabled": true,
            "path": "/var/log/dns_logs.json"
        },
        {
            "type": "http",
            "enabled": false,
            "endpoint": "http://example.com/logs",
            "method": "POST",
            "headers": {
                "Authorization": "Bearer abc123"
            }
        },
        {
            "type": "syslog",
            "enabled": false,
            "address": "127.0.0.1",
            "port": 514,
            "protocol": "udp"
        }
    ]
}
